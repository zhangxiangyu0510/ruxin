/* pages/my/my.wxss */
page {
  min-height: 100%;
  background: #fafafa;
  /* padding-bottom:108rpx; */
}
.weui-dialog__title{
    outline: none !important;
}
.my{
    box-sizing: border-box;
  padding-top: 176rpx;
  min-height: 100vh;
    overflow: hidden;
}
.my_top_header{
    margin-bottom: 40rpx;
}
.header_img{
  position:absolute;
  top:0;
  left:0;
  width:100%;
}
.my_header{
  /* height: 230rpx; */
  width:100%;
  /* background: #FFF; */
  padding:28rpx 32rpx 0 32rpx;
  box-sizing: border-box;
  display: flex;
  position:relative;
}
.my_header_avater{
  height:130rpx;
  width:130rpx;
  border-radius: 8rpx;
  /* background: skyblue; */
  overflow: hidden;
}
.my_header_avater image{
  width: 100%;
  height:100%
}
.my_header_infos{
  /* width:416rpx; */
  /* background: #f1f1f1; */
  height:120rpx;
  margin-left:32rpx;
}
.my_header_infos .toLogin{
  display: flex;
  align-items: center;
  height: 100%;
}
.my_infos_name{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 32rpx;
  line-height: 44rpx;
  color: #000;
  white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
  max-width: 340rpx;
}
.my_infos_icon{
  margin-top:41rpx;
  width:144rpx;
  height:44rpx;
  /* background: skyblue; */
  display: flex;
}
.left_icons image{
  height:44rpx;
  width:44rpx;
}
.left_icons_text{
height:44rpx;
width:106rpx;
border-radius: 4rpx;
font-family: PingFang SC;
font-style: normal;
font-weight: normal;
font-size: 20rpx;
line-height: 44rpx;
text-align: center;
color: #FFFFFF;
position: relative;
}
.left_icons_text image{
  width:100%;

}
.left_text{
  position: absolute;
    top: 0;
    right: 8rpx;
}
.my_header_edit{
  position:absolute;
  height:48rpx;
  width:176rpx;
  /* background-color: red; */
  right:32rpx;
  top:82rpx;
  text-align: center;
  display: flex;

}
.my_edit_icon image{
  height:48rpx;
  width:48rpx;
}
.my_edit_text{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: normal;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #4A4A4A;
  margin-top:4rpx;
  margin-left:16rpx;
}
.become_star_box{
  width:678rpx;
  height:84rpx;
  margin:0 auto;
  margin-top:40rpx;
  margin-bottom:48rpx;
  /* background: #FF5E00; */
  border-radius: 16rpx 16rpx 0px 0px;
  position:relative;
}
.left_top_text{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: normal;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #FFFFFF;
  position: absolute;
  top: 26rpx;
  left: 16rpx;
}
.right_top_box{
  width: 168rpx;
  height: 48rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
  position:absolute;
  right:20rpx;
  top:18rpx;
  display: flex;

}
.right_text{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 20rpx;
  line-height: 28rpx;
  color: #7340B3;
  position:absolute;
  right:26rpx;
  top:10rpx;
}
.right_img{
  width:72rpx;
  height:48rpx;
}
.top_img{
width:100%;
}
.bottom_img{
  position: absolute;
  right:0;
  top:0;
  width:100%;
}

.en_box{
  background: linear-gradient(90deg, #C4D600 0%, #00B5E2 100%);
  border-radius: 16rpx;
  margin:0 32rpx;
  height:104rpx;
  padding:16rpx 32rpx;
  box-sizing: border-box;
  display: flex;
  position:relative;
}
.en_box_img{
  width:114rpx;
  height:72rpx;
}
.my_ens{
  font-family: PingFang SC;
font-style: normal;
font-weight: normal;
font-size: 28rpx;
line-height: 76rpx;
color: #FFFFFF;
margin-left:32rpx;
}
.en_right_box{
  height:40rpx;
  /* width:66rpx; */
  /* background: red; */
  position:absolute;
  right:32rpx;
  top:50%;
  transform: translateY(-50%);
  /* text-align: right; */
  display: flex;
}
.en_right_img{
  height:32rpx;
  width:32rpx;
  margin-left:16rpx;
  margin-top:4rpx;
}
.en_num{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 28rpx;
  line-height: 40rpx;
  text-align: right;
  color: #FFFFFF;
  display: inline-block;
}
.myself_buy_box{
  margin:48rpx 32rpx 0 32rpx;
  /* background: #f1f1f1; */
}
.myself_title{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 600;
  font-size: 40rpx;
  line-height: 56rpx;
  color: #404040;
}
.myself_box{
  background: #FFFFFF;
  border: 4rpx solid #FF5E00;
  box-sizing: border-box;
  border-radius: 16rpx;
  /* height:132rpx; */
  width:100%;
  margin-top:32rpx;
  padding:32rpx;
  display:flex;
  position:relative;
}
.myself_box_logo{
  height:72rpx;
  width:72rpx;
  background: #000;
}
.myself_box_text{
  margin-left:22rpx;
}
.myself_text_top{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #4A4A4A;
}
.myself_text_bottom{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 300;
  font-size: 20rpx;
  line-height: 28rpx;
  color: #4A4A4A;
  opacity: 0.8;
}
.myself_btn{
  width:144rpx;
  height:64rpx;
  background: #FF5E00;
  border-radius: 8rpx;
  position:absolute;
  top:32rpx;
  right:32rpx;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 22rpx;
  line-height: 64rpx;
  color: #FFF;
  text-align: center;
}
.myself_order_box{
  margin:64rpx 0 32rpx 0;
  position: relative;
}
.myself_order_title{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 600;
  font-size: 40rpx;
  line-height: 56rpx;
  color: #404040;
  position:relative;
}
.myself_order_all{
position:absolute;
right:0;
bottom:0;
font-family: PingFang SC;
font-style: normal;
font-weight: normal;
font-size: 24rpx;
line-height: 34rpx;
color: #4A4A4A;
}
.myself_status_box{
  height: 184rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  margin-top:32rpx;
  display: flex;
  padding:32rpx;
  box-sizing: border-box;
  justify-content: space-between;
}
.myself_status_single{
  height:120rpx;
  /* background: #f1f1f1; */
  width:80rpx;
  position:relative;
  text-align: center;
}
.myself_status_single_icon{
  height:80rpx;
  width:80rpx;
  /* background: skyblue; */
  position: absolute;
  top:0;
  left: 50%;
  transform: translateX(-50%);
}
.myself_status_single_icon image{
  width: 100%;
  height: 100%;
}
.myself_status_single_text{
  position: absolute;
  bottom:0;
  left: 50%;
  transform: translateX(-50%);
  width:80rpx;
  font-family: Roboto;
  font-style: normal;
  font-weight: normal;
  font-size: 24rpx;
  line-height: 28rpx;
  color: #000;

}
.coupons_box{
  margin:0 32rpx;
  /* height:416rpx; */
  background: #FFFFFF;
  border-radius: 16rpx;
  margin-top:64rpx;
  padding:0 32rpx;
  box-sizing: border-box;
}
.single_line_box{
  font-family: PingFang SC;
font-style: normal;
font-weight: normal;
font-size: 28rpx;
line-height: 40rpx;
color: #404040;
padding:32rpx 0;
border-bottom: 1rpx solid rgba(0,0,0,0.05);
position:relative;
}
.single_line_box:last-child{
  border-bottom: 1rpx solid transparent;
}
.single_right_arrow{
  height:32rpx;
  width:32rpx;
  position: absolute;
  right:0;
  top:50%;
  transform: translateY(-50%);
}
.single_right_arrow image{
  height:32rpx;
  width:32rpx;
}
.infoSum{
    position: absolute;
    top: -12rpx;
    left: 46rpx;
    width: 36rpx;
    padding-left: 2rpx;
    height: 36rpx;
    background: #FA5151;
    border-radius: 18rpx;
    color: #FFFFFF;
    font-family: 'SF Pro Display';
    font-style: normal;
    font-weight: 500;
    font-size: 26rpx;
    line-height: 36rpx;
    display: flex;
    justify-content: space-around;
}
