/* pages/customer/feedbackComplaintsList/feedbackComplaintsList.wxss */
.list{
    min-height: 100vh;
    background-color: #fafafa;
}
.feedbackComplaintsList{
    padding: 0 32rpx;
    background-color: #fafafa;
    /* padding-top: 176rpx; */
    font-family: PingFang SC;
    font-style: normal;
    padding-top: 48rpx;
    min-height: 88vh;
}
.feedbackComplaintsList .historyList{
    box-sizing: border-box;
    width: 686rpx;
    background-color: #ffffff;
    margin-bottom: 32rpx;
    padding: 32rpx;
    padding-bottom: 8rpx;
    border-radius: 16rpx;
    overflow: hidden;
}
.feedbackComplaintsList .historyList .contentTop{
    display: flex;
    justify-content: space-between;
    font-weight: normal;
    font-size: 12px;
    line-height: 34rpx;
    color: #888888;
    margin-bottom: 24rpx;
}
.feedbackComplaintsList .historyList .contentText{
    box-sizing: border-box;
    font-weight: normal;
    font-size: 14px;
    line-height: 40rpx;
    color: #404040;
    mix-blend-mode: normal;
    padding-bottom: 24rpx;
}
.feedbackComplaintsList .historyList .contentText text{
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
    text-overflow: ellipsis;
}
.feedbackComplaintsList .historyList .img{
    display: flex;
    align-items: center;
    
}
.feedbackComplaintsList .historyList .img image{
    width: 160rpx;
    height: 160rpx;
    margin-bottom: 32rpx;
    margin-right: 32rpx;
    border-radius: 16rpx;
}
.feedbackComplaintsList .historyList .img .goBeyond{
    position: relative;
    right: -32rpx;
    width: 32rpx;
    height: 32rpx;
}
.notData{
    width: 100%;
    text-align: center;
    color: #999999;
    background-color: #fafafa;
    margin-bottom: 60rpx;
}