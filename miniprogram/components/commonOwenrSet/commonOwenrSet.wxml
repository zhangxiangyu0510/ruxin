<view class="shopowenr_all">

  <view class="shopowenr_header">
    <view class="shopowenr_header_logo1"></view>
    <view class="shopowenr_header_info">
      <text class="shopowenr_info1">薛静静呀</text>
      <view class="shopowenr_info2All">
        <view class="shopowenr_info2"><text>45</text>粉丝数</view>
        <view class="shopowenr_info3"><text>1.9</text>年店主</view>
      </view>
    </view>
    <view class="shopowenr_header_right" wx:if="{{ifShow}}">
      <view class="shopowenr_right1" bindtap="goConfig">
        <view class="shopowenr_right1_icon"></view>
        <text class="shopowenr_text">设置人设</text>
      </view>
      <view class="shopowenr_right1" bindtap="goReview">
