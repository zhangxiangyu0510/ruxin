          </view>
          <view class="shopowenr_center1_item">
            <text class="shopowenr_center1_text3">年龄</text>
            <text class="shopowenr_center1_text2">30</text>
          </view>
          <view class="shopowenr_center1_item">
            <text class="shopowenr_center1_text3">星座</text>
            <text class="shopowenr_center1_text2">天蝎座</text>
          </view>
          <view class="shopowenr_center1_item">
            <text class="shopowenr_center1_text3">年资</text>
            <text class="shopowenr_center1_text2">2年</text>
          </view>
        </view>
        <view class="shopowenr_center1Box2">
          <view class="shopowenr_center1_item">
            <view class="shopowenr_center1_img1"></view>
            <text class="shopowenr_center1_text4">123456789</text>
          </view>
          <view class="shopowenr_center1_item">
            <view class="shopowenr_center1_img1"></view>

            <text class="shopowenr_center1_text4">123456789</text>
          </view>
          <view class="shopowenr_center1_item">
            <view class="shopowenr_center1_img1"></view>

            <text class="shopowenr_center1_text4">123456789</text>
          </view>
          <view class="shopowenr_center1_item">
            <view class="shopowenr_center1_img1"></view>
            <text class="shopowenr_center1_text4">12345678909</text>
          </view>
          <view class="shopowenr_center1_item">
            <view class="shopowenr_center1_img1"></view>
            <text class="shopowenr_center1_text4">12345678909</text>
          </view>
        </view>
      </view>

      <view class="shopowenr_center1_bottom">
        <view class="shopowenr_center1_bottom_img"></view>
        <text>中国上海市</text>
      </view>
    </view>


  </view>


  <view class="shopowenr_center1_fu" wx:if="{{ifShow}}">
    <view class="shopowenr_center1_header">

      <text class="shopowenr_center1_text1">销售贡献</text>
      <text  class="shopowenr_center1_text6">仅限NU店，业绩查询请至星享城</text>
    </view>
    <view class="shopowenr_center2_body">

      <view class="shopowenr_center2_body_item">

        <view class="shopowenr_center2_body_text1">29</view>
        <view class="shopowenr_center2_body_text2">累计订单数</view>
        <view class="shopowenr_center2_body_text3">提示1文案待定</view>


      </view>
      <view class="shopowenr_center2_body_item">
        <view class="shopowenr_center2_body_text1"><text>￥</text>28419</view>
