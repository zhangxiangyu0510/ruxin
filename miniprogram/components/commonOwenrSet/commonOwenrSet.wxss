page {
  min-height: 100%;
  /* width: 100%; */
  box-sizing: border-box;
  padding: 32rpx 32rpx 0;
  background: #fafafa;
  /* padding-bottom:108rpx; */
}
.shopowenr_all{
  margin-bottom: 44rpx;
  overflow: hidden;
}


.shopowenr_header {
  width: 100%;
  /* height: 140rpx; */
  overflow: hidden;

}

.shopowenr_header_logo1 {
  width: 100rpx;
  height: 100rpx;
  background-color: #FF5E00;
  border-radius: 50%;
  /* margin-top: 30rpx; */
  float: left;
}

.shopowenr_header_info {
  float: left;
  margin-left: 24rpx;
}

.shopowenr_info1 {
  color: #000;
  font-size: 32rpx;
  font-weight: bold;
  line-height: 44rpx;
}

.shopowenr_info2,
.shopowenr_info3 {
  font-size: 28rpx;
  float: left;
  line-height: 38rpx;
  color: #7F7F7F;
}

.shopowenr_info3 {
  margin-left: 48rpx;
}

.shopowenr_info2 text,
.shopowenr_info3 text {
  color: #4A4A4A;
  margin-right: 8rpx;
}

.shopowenr_info2All {
  margin-top: 16rpx;
}

.shopowenr_header_right {
  width: 235rpx;
  float: right;
  color: #4A4A4A;
}

.shopowenr_right1_icon {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background-color: #FF5E00;
  text-align: center;
  margin: 0 auto 20rpx;
}

.shopowenr_right1 {
  width: 100rpx;
  float: left;
  font-size: 24rpx;
  line-height: 36rpx;
