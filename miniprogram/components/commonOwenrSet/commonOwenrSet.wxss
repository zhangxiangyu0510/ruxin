  height: 84rpx;
  line-height: 84rpx;
  font-size: 28rpx;
  background-color: #FF5E00;
  border-top-left-radius: 16rpx;
  border-top-right-radius: 16rpx;
  text-indent: 32rpx;
  color: #ffffff;
}

.shopowenr_header1 view text {
  padding-left: 18rpx;
  padding-right: 5rpx
}

