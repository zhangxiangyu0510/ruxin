.common_dialog_bg{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.6);
    z-index: 999999;
  }
  .addInfo_dialog{
    position: relative;
    height:100%;
    width:100%;
  }
  .common_close_btn{
    height:35rpx;
    width:35rpx;
    position: absolute;
    /* background-color: skyblue; */
    right:120rpx;
    top:22%;
    transform: translateY(-50%);
    z-index:1000000;
  }
  .addInfo_dialog_content_box_bg{
    position:absolute;
    width: 496rpx;
    /* height: 723rpx; */
    background: #FFF;
    border-radius: 16rpx;
    left:50%;
    top:50%;
    transform: translate(-50%,-50%);
  }
  .addInfo_dialog_content_box{
    position:relative;
    height:100%;
    width:100%;
    padding:80rpx 48rpx 48rpx 48rpx;
    box-sizing: border-box;
  }
  .addInfo_dialog_con_logo{
    height:272rpx;
    width:346rpx;
    /* background: skyblue; */
    margin:0 auto;
  }
  .addInfo_dialog_con_logo image{
    height:272rpx;
    width:346rpx;
    display: block;
  }
  .addInfo_dialog_con_text{
    font-family: PingFang SC;
    font-style: normal;
    font-weight: 600;
    font-size: 48rpx;
    line-height: 68rpx;
    color: #404040;
    text-align: center;
    margin:48rpx 0;
    overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
  }
  .addInfo_dialog_sub_text{
    font-family: PingFang SC;
    font-style: normal;
    font-weight: normal;
    font-size: 28rpx;
    line-height: 40rpx;
    height:40rpx;
    text-align: center;
    color: #404040;
  }
  .addInfo_dialog_btn{
    border-radius: 8rpx;
    width:100%;
    height:84rpx;
    margin-top: 56rpx;
    font-family: PingFang SC;
    font-style: normal;
    font-weight: 600;
    font-size: 28rpx;
    line-height: 84rpx;
    text-align: center;
    color: #FFF;
  }