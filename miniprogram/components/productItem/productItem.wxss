}

.productItem_box_left {
  float: left;
}

.productItem_box_right {
  float: right;
  width: 40rpx;
  height: 40rpx;
  /* background-color: #7340B3; */
  padding: 14rpx;
  margin-right: 10rpx;
  margin-top: 34rpx;
  position: relative;
  left: 24rpx;
}

.HonourEnjoyCodeModel{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.7);
    z-index: 9999;

}
.HonourEnjoyCodeModel .modelBox{
  position: absolute;
  bottom: 0;
  
  border-radius: 30rpx;
  background: linear-gradient(180deg, #ecfbff 0%, #fff 100%);
  padding: 30rpx;
  z-index: 2;
  

}
.HonourEnjoyCodeModel .modelBox .title{
  font-size: 32rpx;
  color: #0e82ad;
  text-align: center;
  margin-bottom: 30rpx;
  font-weight: 600;
  margin-top: 20rpx;

}
.HonourEnjoyCodeModel .modelBox .text{
  font-size: 28rpx;
  color: #0e82ad;


}
.HonourEnjoyCodeModel .modelBox  .hilt{
  text-align: center;
  font-size: 24rpx;
  font-weight: 300;
  color: #a6a9b5;
  margin-bottom: 60rpx;
}
.HonourEnjoyCodeModel .modelBox .button{
  background: linear-gradient(90deg, #494396 0%, #38add4 100%);
  border-radius: 50rpx;
  padding: 24rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  margin-bottom: 40rpx;
}
.HonourEnjoyCodeModel .modelBox .selectBox{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 24rpx ;
  border-radius: 16rpx;
  border: 1px solid #5fdcff;
  color: #4a4946;
  font-size: 28rpx;
  margin-top: 50rpx;
  margin-bottom: 60rpx;
background-color: #fff;
  

}
.HonourEnjoyCodeModel .modelBox  .icon{
  display: block;
  width: 0;
  height: 0;
  border:16rpx solid transparent;
  border-top-color:#d9d9d9;
  position: relative;
  top: 8rpx;
}
.HonourEnjoyCodeModel .modelBox  .name{
  width: 590rpx;
  height: auto;
  word-wrap: break-word; /*强制换行*/
  overflow: hidden; /*超出隐藏*/
  text-overflow: ellipsis;/*隐藏后添加省略号*/
  white-space: nowrap;/*强制不换行*/



}
.HonourEnjoyCodeModel .codeList{
  border-radius: 30rpx;
  position: absolute;
  bottom: 0;
  z-index: 3;
  background-color: #fff;
  box-sizing: border-box;

}
.HonourEnjoyCodeModel .codeList .listBox{
  max-height: 450rpx;
  overflow-y: scroll;
}
.HonourEnjoyCodeModel .codeList .listData{
  width: 690rpx;
  padding: 30rpx;
  color: #333333;
  font-size: 26rpx;
  text-align: center;
  margin: 0 auto;
  border-bottom: 10rpx solid #f5f5f5; 
  word-wrap: break-word; /*强制换行*/
  overflow: hidden; /*超出隐藏*/
  text-overflow: ellipsis;/*隐藏后添加省略号*/
  white-space: nowrap;/*强制不换行*/


}
.HonourEnjoyCodeModel .codeList .close{
  width: 750rpx;
  display: block;
  text-align: center;
  padding: 25rpx;
  margin-bottom: 50rpx;
  background-color: #fff;
  box-sizing: border-box;
  color: #333333;

}