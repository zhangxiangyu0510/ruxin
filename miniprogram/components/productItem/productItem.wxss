}

.productItem_box_left1_text4 {
  font-family: Roboto;
  font-style: normal;
  font-weight: normal;
  font-size: 28rpx;
  line-height: 32rpx;
  float: left;
  margin-left: 16rpx;
  /* identical to box height */

