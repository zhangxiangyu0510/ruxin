}

.productItem_box_left2 {
  height: 32rpx;
  margin-top: 10rpx;

}

.productItem_box_left1_text3 {
  height: 32rpx;
  /* width: 72rpx; */
  text-align: center;
  /* background-color: #7340B3; */
  font-family: PingFang SC;
  font-style: normal;
  font-weight: normal;
  font-size: 20rpx;
  line-height: 32rpx;
  border-radius: 4rpx;
  float: left;
  padding: 0 6rpx;

  /* identical to box height, or 100% */

